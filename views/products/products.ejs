<!DOCTYPE html>
<html>
  <head>
    <title>Produce Stand CRUD</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  <h1><%= category %> products</h1>
  <p><a class="btn btn-primary" href="/">Back</a></p>
  <ul>
    <% for(let product of products) { %>
      <li> 
        <%= product.name %> 
        <a class="display-5" href="/edit/<%= product.id %>">Edit</a> 
        <a class="display-5"href="/products/<%= product.id %>">View</a>
        <a class="display-5" href="/delete/<%= product.id %>">Delete</a>
      </li>
    <% } %>
  </ul>
  <a class="display-3 href="/new">Add Product</a>
  </body>
</html>
